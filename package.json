    "repository": {
        "type": "git",
        "url": "git+https://github.com/geometryzen/jsxmath.git"
    },
    "keywords": [
        "algebrite",
        "algebra",
        "blade",
        "cas",
        "clojurescript",
        "computer",
        "eigenmath",
        "geometric",
        "math",
        "multivector",
        "symbolic",
        "system"
