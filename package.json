        "unit-coverage": "mocha 'units/coverage.spec.ts'",
        "unit-d": "mocha 'units/d.spec.ts'",
        "unit-defint": "mocha 'units/defint.spec.ts'",
        "unit-derivative": "mocha 'units/derivative.spec.ts'",
